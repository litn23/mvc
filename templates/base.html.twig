        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="{{ asset('img/glider.svg') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        <header class="site-header" style="background-image: url({{ asset('img/cherry.jpg') }})">
            <span class="site-title">My Symfony Site</span>
        </header>
        <nav class="navbar">
            <ul>
                <li><a href="{{ path('home') }}">Home</a></li>
                <li><a href="{{ path('me') }}">Me</a></li>
                <li><a href="{{ path('about') }}">About</a></li>
                <li><a href="{{ path('report') }}">Report</a></li>
                <li><a href="{{ path('lucky') }}">Lucky</a></li>
                <li><a href="{{ path('api') }}">Api</a></li>
            </ul>
        </nav>
        <main>
        {% block body %}{% endblock %}
        </main>

        <footer>
            <div class="row">
                <div class="col1-final">
                    <p>Denna sidan Ã¤r Copyright &copy; av litn23.</p>
                </div>
            </div>
